{
	"semi": false,
	"trailingComma": "all",
	"singleQuote": true,
	"printWidth": 80,
	"tabWidth": 3,
	"tabSize": 3,
	"arrowParens": "always",
	"bracketSpacing": true,
	"endOfLine": "lf",
	"htmlWhitespaceSensitivity": "css",
	"insertPragma": false,
	"singleAttributePerLine": false,
	"bracketSameLine": false,
	"jsxBracketSameLine": false,
	"jsxSingleQuote": true,
	"proseWrap": "preserve",
	"quoteProps": "as-needed",
	"requirePragma": false,
	"useTabs": true,
	"vueIndentScriptAndStyle": false
}
